<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CategoryDao">

	<select id="selectOne" resultType="categoryVO" parameterType="string">
	select category_no , category_name from category where category_name = #{category_name}
	</select>
	
	<select id="selectNum" resultType="categoryVO" parameterType="string">
	select category_no , category_name from category where category_no = #{category_no}
	</select>
	
    <select id="selectList" resultType="categoryVO">
    select * from category
	</select>
</mapper>



